import math
from time import time

def timedec(func):
    def timefunc(*args):
        start = time()
        func(*args)
        print(f'Время работы функции:{time() - start}')
    return timefunc

def decor(func):
    def name(*args):
        print(f'Была вызвана функция {func.__name__}')
        func(*args)
    return name

@timedec
@decor

def formula(a, d, Vi):
    Vf = math.sqrt(Vi ** 2 + 2 * a * d)
    print('Финальная скорость:',round(Vf,1),'м/с')

a = 0
float(a)
a = 9.8
Vi = 0
float (Vi)
d = float(input('Введите высоту в метрах: '))

formula(a, d, Vi)

